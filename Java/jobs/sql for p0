CREATE TABLE bankuser_junction(
   account_id NUMBER NOT NULL,
    bankuser_id NUMBER NOT NULL,
    CONSTRAINT my_combo_key PRIMARY KEY
        (account_id, bankuser_id),
    FOREIGN KEY (my_bankuser_id) REFERENCES ACCOUNTS (bankuser_id),
    FOREIGN KEY (my_account_id) REFERENCES  BANK_USERS (account_id)
);


CREATE TABLE bankuser_junction(
   account_id NUMBER NOT NULL,
    bankuser_id NUMBER NOT NULL,
    CONSTRAINT my_combo_key PRIMARY KEY
        (account_id, bankuser_id),
    FOREIGN KEY (account_id) REFERENCES superhumans (bankuser_id),
    FOREIGN KEY (bankuser_id) REFERENCES bankuser (account_id)
);

