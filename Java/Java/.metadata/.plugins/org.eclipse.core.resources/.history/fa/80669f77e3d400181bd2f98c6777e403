/**
 * @author Jae
 *
 */
import java.util.*;
public class RockPaperScissors {
	private int computerWin = 0;
	private int userWin = 0;
	private static Choice computerChoice;


	private enum Choice
	{ 
	    ROCK, PAPER, SCISSORS; 
	} 
	
	
	public Character userChoice() {
		 @SuppressWarnings("null")
		Character userChoice = null;
		 Scanner input = new Scanner(System.in);
		 System.out.print("Enter r for rock, p for paper, or s for scissors");
		
		 userChoice=Character.toLowerCase(input.next().charAt(0));
		 input.close();
		 
			
		return userChoice;
	}
	public static Choice computerChoice() {
		
		
		int choiceValue =  (int) ((int) 3 * Math.random());
		
		switch(choiceValue) {
		case (0): computerChoice = Choice.ROCK; 
		break;
		
		case (1): computerChoice = Choice.PAPER; 
		 break;
		
		case (2): computerChoice = Choice.SCISSORS; 
		break;
		
		default: computerChoice = Choice.ROCK; 
		break;
			
		
		}
		return computerChoice; 
		
	};
	
	public void determineWinner() {
		RockPaperScissors Game = new RockPaperScissors();
		System.out.println(Game.userChoice());
		Character userChose = Game.userChoice();
		Choice computerChose = computerChoice();
		
		
		
		userChoice()
		if ((computerChose == Choice.ROCK && userChose == 'r')|| (computerChose == Choice.PAPER && userChose == 'p')|| (computerChose == Choice.SCISSORS && userChose == 'p')
				//play agaIN
		if((computerChose == Choice.ROCK && userChose == 's')|| (computerChose == Choice.PAPER && userChose == 'r')|| (computerChose == Choice.SCISSORS && userChose == 'p')
				computerWin++;
		if((computerChose == Choice.ROCK && userChose == 'p')|| (computerChose == Choice.PAPER && userChose == 's')|| (computerChose == Choice.SCISSORS && userChose == 'r')
				userWin++;
		
				
		
		
	}

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println(RockPaperScissors.computerChoice());
	

	}

}
