 */

/**
 * @author Jae
 *
 */
public class RockPaperScissors {
	
	enum Choice
	{ 
	    ROCK, PAPER, SCISSORS; 
	} 
	
	public static enum computerChoice() {
		double int choiceValue = (int) 3 * Math.random();
		switch(choiceValue) {
		case (0) return ROCK; break;
		case (1) return PAPER; break;
		case (2) return SCISSORS; break;
		
		}
		}
		
	};

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println(RockPaperScissors.computerChoice());

	}

}
